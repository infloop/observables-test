{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-bitwise": false,
    "no-trailing-whitespace": false,
    "no-namespace": false,
    "no-console": false,
    "member-access": false,
    "interface-name": true,
    "trailing-comma": false,
    "max-line-length": [
      true,
      140
    ],
    "interface-name": false
  },
  "member-ordering": [
    true,
    "public-before-private",
    "static-before-instance",
    "variables-before-functions"
  ]
}
